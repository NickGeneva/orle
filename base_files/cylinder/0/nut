FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wall
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    cylinder
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    jet1
	{
		type            calculated;
        value           uniform 0;
	}	
	jet2
	{
		type            calculated;
        value           uniform 0;
    }
    jet3
	{
		type            calculated;
        value           uniform 0;
	}	
    jet4
	{
		type            calculated;
        value           uniform 0;
	}
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    cyclic1
    {
        type            cyclic;
    }
    cyclic2
    {
        type            cyclic;
    } 
}


