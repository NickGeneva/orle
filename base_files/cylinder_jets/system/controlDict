    forces_cylinder
    {
        type forces;
        functionObjectLibs ("libforces.so");
        enabled true;
        writeControl timeStep;
        writeInterval 100;
        patches
        (
            cylinder jet1 jet2 jet3 jet4
        );
        log true;
        rho rhoInf;
        rhoInf 1.0;
        CofR (0 0 0);
        liftDir (0 1 0);
        dragDir (1 0 0);
        pitchAxis (0 0 1);
        magUInf 0.9756;
        lRef 0.1;
        Aref 0.001;
    }

